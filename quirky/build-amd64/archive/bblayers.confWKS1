# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ABSPATH/oe-quirky/meta \
  ABSPATH/oe-quirky/meta-poky \
  ABSPATH/oe-quirky/meta-yocto-bsp \
  ABSPATH/oe-quirky/meta-multimedia \
  ABSPATH/oe-quirky/meta-oe \
  "

